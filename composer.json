{
    "name": "allanmcarvalho/tools",
    "description": "Tools plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.3.2 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Tools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tools\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
